{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/dotfiles",
      "destination": "https://github.com/redreceipt/dotfiles"
    },
    {
      "source": "/edit",
      "destination": "https://gitpod.io/#github.com/redreceipt/neeley-dev"
    },
    {
      "source": "/new",
      "destination": "https://gitpod.io/#github.com/redreceipt/new"
    },
    {
      "source": "/box",
      "destination": "https://www.dropbox.com/s/id5hgd6097br1yt/init.sh"
    }
  ]
}
